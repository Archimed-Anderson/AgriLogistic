{
  "name": "agrologistic-platform",
  "description": "Plateforme SaaS complète pour la chaîne d'approvisionnement agricole",
  "version": "2.0.1",
  "packageManager": "pnpm@9.0.0",
  "private": true,
  "type": "module",
  "author": "AgroLogistic Team <dev@agrologistic.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agrologistic/agrologistic-platform.git"
  },
  "bugs": {
    "url": "https://github.com/agrologistic/agrologistic-platform/issues"
  },
  "homepage": "https://agrologistic.com",
  "keywords": [
    "agriculture",
    "saas",
    "marketplace",
    "logistics",
    "iot",
    "clean-architecture",
    "react",
    "typescript"
  ],
  "workspaces": [
    "services/*/*",
    "packages/*",
    "infrastructure/api-gateway"
  ],
  "scripts": {
    "dev": "turbo run dev --concurrency=25 --filter=!@agrologistic/auth-service --filter=!@agrologistic/ai-service",
    "dev:host": "turbo run dev --filter=!@agrologistic/auth-service -- --host",
    "dev:web-app": "turbo run dev --filter=@agrologistic/web-app",
    "services:dev": "turbo run dev --filter=./services/*/*",
    "services:build": "turbo run build --filter=./services/*/*",
    "services:test": "turbo run test --filter=./services/*/*",
    "services:lint": "turbo run lint --filter=./services/*/*",
    "web-app:dev": "turbo run dev --filter=@agrologistic/web-app",
    "web-app:build": "turbo run build --filter=@agrologistic/web-app",
    "build": "vite build",
    "build:all": "turbo run build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "lint": "turbo run lint",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage",
    "test:e2e": "playwright test tests/e2e/smoke.spec.ts --project=chromium",
    "test:e2e:full": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky install",
    "clean": "node scripts/utils/clean.cjs",
    "validate": "pnpm run typecheck && pnpm run lint && pnpm run format:check && pnpm run test:ci",
    "validate:env": "node scripts/validate-env.js",
    "check:health": "node scripts/check-health.js",
    "check:all": "node scripts/validate-all-prompts.js",
    "test:all": "node scripts/run-all-tests.js",
    "pre-deploy": "node scripts/pre-deploy-check.js",
    "analyze:homepage": "node scripts/run-homepage-analysis.js",
    "test:login": "node scripts/run-login-tests.js",
    "test:modern-login": "node scripts/run-modern-login-tests.js",
    "lighthouse": "node scripts/lighthouse-audit.js",
    "lint:quiet": "eslint . --ext ts,tsx --quiet",
    "validate:services": "powershell -ExecutionPolicy Bypass -File scripts/validate-health-endpoints.ps1",
    "pre-production": "node scripts/pre-production-check.js",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run tests/integration --reporter=verbose",
    "validate:full": "pnpm run typecheck && pnpm run lint:quiet && pnpm run test:unit && pnpm run build:all",
    "setup": "bash setup.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "1.2.3",
    "@radix-ui/react-alert-dialog": "1.1.6",
    "@radix-ui/react-aspect-ratio": "1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "1.1.4",
    "@radix-ui/react-collapsible": "1.1.3",
    "@radix-ui/react-context-menu": "2.2.6",
    "@radix-ui/react-dialog": "1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "1.1.6",
    "@radix-ui/react-navigation-menu": "1.2.5",
    "@radix-ui/react-popover": "1.1.6",
    "@radix-ui/react-progress": "1.1.2",
    "@radix-ui/react-radio-group": "1.2.3",
    "@radix-ui/react-scroll-area": "1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "1.1.2",
    "@radix-ui/react-slider": "1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "1.1.3",
    "@radix-ui/react-tabs": "1.1.3",
    "@radix-ui/react-toggle": "1.1.2",
    "@radix-ui/react-toggle-group": "1.1.2",
    "@radix-ui/react-tooltip": "1.1.8",
    "@tanstack/react-query": "^5.90.19",
    "@turf/turf": "^7.3.3",
    "@types/leaflet": "^1.9.21",
    "@types/mapbox-gl": "^3.4.1",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "8.6.0",
    "html2canvas": "^1.4.1",
    "idb": "^8.0.3",
    "input-otp": "1.4.2",
    "jspdf": "^4.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.487.0",
    "mapbox-gl": "^3.18.1",
    "maplibre-gl": "^5.16.0",
    "motion": "12.23.24",
    "react": "18.3.1",
    "react-day-picker": "8.10.1",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.3.1",
    "react-force-graph-2d": "^1.29.0",
    "react-hook-form": "^7.53.0",
    "react-leaflet": "^5.0.0",
    "react-map-gl": "^8.1.0",
    "react-resizable-panels": "2.1.7",
    "react-router-dom": "^7.13.0",
    "react-window": "^2.2.5",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "1.1.2",
    "zod": "3.24.1",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/vite": "4.1.12",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.10.7",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "@vitejs/plugin-react": "4.7.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "eslint": "8.56.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "husky": "8.0.3",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "prettier": "3.0.3",
    "tailwindcss": "4.1.12",
    "turbo": "^2.7.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.3.3",
    "vite": "^6.4.1",
    "vitest": "^4.0.17"
  },
  "pnpm": {
    "overrides": {
      "vite": "6.3.5",
      "zod": "3.24.1"
    }
  }
}
