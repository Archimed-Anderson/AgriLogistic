{
  "name": "agrologistic-platform",
  "description": "Plateforme SaaS complète pour la chaîne d'approvisionnement agricole",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "author": "AgroLogistic Team <dev@agrologistic.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agrologistic/agrologistic-platform.git"
  },
  "bugs": {
    "url": "https://github.com/agrologistic/agrologistic-platform/issues"
  },
  "homepage": "https://agrologistic.com",
  "keywords": [
    "agriculture",
    "saas",
    "marketplace",
    "logistics",
    "iot",
    "clean-architecture",
    "react",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky install",
    "clean": "rm -rf dist node_modules .vite",
    "validate": "npm run typecheck && npm run lint && npm run format:check && npm run test:ci",
    "validate:env": "node scripts/validate-env.js",
    "check:health": "node scripts/check-health.js",
    "test:all": "node scripts/run-all-tests.js",
    "pre-deploy": "node scripts/pre-deploy-check.js",
    "analyze:homepage": "node scripts/run-homepage-analysis.js",
    "test:login": "node scripts/run-login-tests.js",
    "test:modern-login": "node scripts/run-modern-login-tests.js",
    "lighthouse": "node scripts/lighthouse-audit.js"
  },
